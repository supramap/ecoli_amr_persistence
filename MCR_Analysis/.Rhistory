## Create a function to compare the rule vs. the genotype sets
comparefxn <- function(a, b){
if(setequal(a, unlist(b))){
response <- "Match"
} else if(all(a %in% b)){
response <- "Subset"
} else if(all(b %in% a)){
response <- "Superset"
} else{
response <- NA
}
return(response)
}
